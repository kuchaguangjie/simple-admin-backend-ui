    "lint-staged": "13.2.2",
    "prettier-plugin-packagejson": "^2.4.7",
    "rimraf": "^5.0.5",
    "turbo": "^1.11.2",
    "typescript": "^5.3.3",
    "unbuild": "^1.2.1",
    "unplugin-vue-define-options": "^1.4.1",
    "vite": "^4.5.1",
